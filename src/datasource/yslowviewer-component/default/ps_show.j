"body":"<?cs def:b2k(size)?><?cs  var:size\/#1024 ?>.<?cs  var:((size*#10)\/#1024)%10 ?><?cs \/def?>\n<?cs def:s2rank(score)?><?cs if:score==\"disabled\" ?>n\/a<?cs elif:#score>=#90 ?>A<?cs elif:#score>=#80 ?>B<?cs elif:#score>=#70 ?>C<?cs elif:#score>=#60 ?>D<?cs elif:#score>=#50 ?>E<?cs else ?>F<?cs \/if ?><?cs \/def?>\n<table>\n  <tbody>\n    <tr class=\"summary\">\n      <th colspan=\"2\">\n\tOverall: <b><?cs var: A.yslowviewer.pageStats.overallScore ?><\/b> &nbsp;&nbsp; PageLoad: <b><?cs var: A.yslowviewer.pageStats.pageLoadTime ?>ms<\/b> &nbsp;&nbsp; PageSize: <?cs call:b2k(A.yslowviewer.pageStats.pageSize) ?>KB &nbsp;&nbsp; Transfer: <?cs call:b2k(A.yslowviewer.pageStats.transferSize) ?>KB  &nbsp;&nbsp; NumRequests: <?cs var: A.yslowviewer.pageStats.numRequests ?>\n      <\/th>\n    <\/tr>\n    <tr class=\"head\">\n      <th class=\"score\">\n\tscore\n      <\/th>\n      <th>\n\tremarks\n      <\/th>\n    <\/tr>\n    <?cs each: item=A.yslowviewer.rules ?>\n    <tr class=\"\">\n      <td class=\"score\">\n\t<div class=\"grade-<?cs call:s2rank(item.score)?>\">\n \t  <?cs var: item.score ?>\t\n\t<\/div>\n      <\/td>\n      <td>\n\t<p class=\"<?cs if:item.@warnings?>warnings<?cs \/if?>\"><?cs var: item.name ?><\/p>\n\t<div class=\"warnings hide\">\n\t  <?cs var: item.@warnings ?>\n\t<\/div>\n      <\/td>\n    <\/tr>\n    <?cs \/each ?>\n  <\/tbody>\n<\/table>\n",
