"css":"#ps_show .hide {\n display:none;\n}\n\n#ps_show table {\n  border-spacing: 0;\n  font-size: 10pt;\n  width: 100%;\n  text-align: left;\n  border: 2px solid #676767;\n  border-width: 2px 2px 1px 1px;\n}\n\n#ps_show table > tbody > tr > th {\n border: solid #676767;\n border-width: 1px 0 0 1px;\n vertical-align: top;\n}\n#ps_show table > tbody > tr > td {\n border: solid #676767;\n border-width: 1px 0 0 1px;\n padding: 2px 5px;\n width: auto;\n text-align: left;\n vertical-align: top;\n}\n#ps_show table > tbody > tr > th.score {\n}\n#ps_show table > tbody > tr > td.score {\n width: 30px;\n text-align: center;\n override:hidden;\n padding-top: 0;\n padding-left: 0;\n}\n\n#ps_show table > tbody > tr.summary {\n background-color: #DFDFDF;\n}\n\n#ps_show table > tbody > tr.summary > th > b {\n color: #0000FF;\n}\n\n\n#ps_show table > tbody > tr > td.score > div {\n border-width:0 0 0 10px;\n border-style:solid;\n font-weight:bold;\n border-color: #676767;\n padding-bottom:9000px;\n margin-bottom:-9000px;\n}\n#ps_show table > tbody > tr > td > div.grade-A {\n border-color: #34a234;\n}\n\n#ps_show table > tbody > tr > td > div.grade-B {\n border-color: #a4cb58;\n}\n\n#ps_show table > tbody > tr > td > div.grade-C {\n border-color: #fadd3d;\n}\n\n#ps_show table > tbody > tr > td > div.grade-D {\n border-color: #f5a249;\n}\n\n#ps_show table > tbody > tr > td > div.grade-E {\n border-color: #e46648;\n}\n\n#ps_show table > tbody > tr > td > div.grade-F {\n border-color: #df4444;\n}\n\n#ps_show table > tbody > tr > td > p {\n font-weight:bold;\n margin:  5px 0 5px 5px;\n}\n#ps_show table > tbody > tr > td > p.warnings:hover {\n font-size: 1.2em;\n color: #0000FF;\n text-decoration: underline;\n cursor:pointer;\n}\n",
"js":"$(function(){\n  $('#ps_show table > tbody > tr > td > p.warnings').click(function(){\n    var CONTENT = $(this).next();\n    if (  CONTENT.hasClass('hide') ) {\n      CONTENT.removeClass('hide');\n    }else{\n      CONTENT.addClass('hide');\n    }\n  });\n});\n",
